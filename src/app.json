{
  "expo": {
    "name": "TutorPug",
    "description": "A find a tutor app",
    "slug": "TutorPug",
    "privacy": "public",
    "sdkVersion": "21.0.0",
    "version": "1.0.0",
    "primaryColor": "#0F5A43",
    "orientation": "portrait",
    "icon": "./assets/icons/app.png",
    "loading": {
      "icon": "./assets/icons/loading.png",
      "hideExponentText": true,
      "backgroundColor": "#0F5A43"
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "karthik-u0",
            "project": "tutorpug",
            "authToken": ""
          }
        }
      ]
    }
  }
}
